    "tailwind-merge": "^3.3.1",
    "xterm": "^5.3.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.0",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
